package com.experion.entity;

public class Account extends Product {
	private String accountType;
    private Bankservices bankservice;
	public Account(String productCode, String productName, String productType, String accountType,Bankservices bankService) {
		super(productCode, productName, productType);
		this.accountType = accountType;
		this.bankservice = bankService;
	}

	public String getAccountType() {
		return accountType;
	}

	public void setAccountType(String accountType) {
		this.accountType = accountType;
	}

	public Bankservices getBankservice() {
		return bankservice;
	}

	public void setBankservice(Bankservices bankservice) {
		this.bankservice = bankservice;
	}
	
	

}
